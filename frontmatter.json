{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "publishedOn",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Cover Image Layout",
          "name": "coverLayout",
          "type": "choice",
          "choices": ["fullscreen", "half", "third", "below-title"]
        },
        {
          "title": "Cover Image",
          "name": "cover",
          "type": "image"
        },
        {
          "title": "Cover Alt",
          "name": "coverAlt",
          "type": "string"
        },
        {
          "title": "Cover Caption",
          "name": "coverCaption",
          "type": "string"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "astro",
  "frontMatter.content.publicFolder": "src/assets",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "test"
  ],
  "frontMatter.taxonomy.categories": [],
  "frontMatter.experimental": true
}