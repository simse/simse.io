---
const { content } = Astro.props;

import Layout from "./Layout.astro";

const breadcrumbs = [
	{
		name: 'Projects',
        path: '/projects'
	},
    {
        name: content.title,
        path: `/projects/${content.slug}`
    }
]
---
<Layout title={content.title} breadcrumbs={breadcrumbs}>
    <slot />
</Layout>